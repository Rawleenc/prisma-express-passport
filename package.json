{
  "name": "prisma-express-passport",
  "version": "1.0.0",
  "description": "A simple RESTful API, utilizing Express with Prisma",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "p:check": "prettier --check ./**/*.{js,json,ts}",
    "p:write": "prettier --write ./**/*.{js,json,ts}",
    "seed": "prisma db seed --preview-feature",
    "migrate": "prisma migrate dev --preview-feature",
    "docker:build": "docker build -t rest-api .",
    "docker:dev": "docker run -d --rm --name Restful-API -p 3000:3000 --env-file .env rest-api yarn start",
    "docker:test": "docker run -d --rm --name Restful-API -p 3000:3000 --env-file .env rest-api yarn test -i",
    "docker-compose:dev": "docker-compose run server",
    "docker-compose:test": "docker-compose -f docker-compose.test.yml run server",
    "dev": "ts-node-dev src/index.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dankphetamine/prisma-express-passport.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dankphetamine/prisma-express-passport/issues"
  },
  "homepage": "https://github.com/dankphetamine/prisma-express-passport#readme",
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "argon2": "^0.27.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "joi": "^17.4.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "prisma": "^2.19.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.21",
    "@types/joi": "^17.2.3",
    "@types/node": "^14.14.25",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.2.3"
  }
}
